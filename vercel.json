{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "domains": [
    "mc-hub.site",
    "www.mc-hub.site"
  ],
  "redirects": [
    {
      "source": "https://www.mc-hub.site/(.*)",
      "destination": "https://mc-hub.site/$1",
      "permanent": true
    }
  ]
}